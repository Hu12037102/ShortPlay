<?xml version="1.0" encoding="utf-8"?>

                                                  
                                                       
                                          

    

           

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
         android:tag="layout/main_acitivity_tiktok_web_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto" tools:ignore="MissingDefaultResource">

        <WebView
            android:id="@+id/web_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/black"
            />


        <com.ruffian.library.widget.RTextView
            android:id="@+id/urge_tv"
            android:layout_width="160dp"
            android:layout_height="40dp"
            app:corner_radius="8dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="117dp"
            android:textSize="15sp"
            android:fontFamily="@font/sofiapro_bold"
            android:gravity="center"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp" />

        <ImageView
            android:id="@+id/coverImageUrl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="44dp">

            <TextView
                android:id="@+id/return_iv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:drawableStart="@drawable/return_logo"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:layout_marginStart="18dp"
                tools:ignore="UseCompatTextViewDrawableXml" />

        </RelativeLayout>


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/aiv_loading"
            android:layout_width="@dimen/loading_width"
            android:layout_height="@dimen/loading_height"
            android:background="@drawable/loading"
            android:visibility="gone"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            tools:visibility="gone" />

    </RelativeLayout>
         