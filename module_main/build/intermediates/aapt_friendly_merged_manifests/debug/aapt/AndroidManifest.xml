    <application>

        <!-- android:theme="@style/Theme.App.Starting" -->
        <activity
            android:name="com.example.module_main.ui.splash.SplashActivity"
            android:autoVerify="true"
            android:exported="true"
            android:theme="@style/Theme.Splash"
            tools:targetApi="m" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter
                android:autoVerify="true"
                tools:targetApi="m" >
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="cct.com.smedia.funreels"
                    android:scheme="fbconnect" />
            </intent-filter>
            <intent-filter
                android:autoVerify="true"
                tools:targetApi="m" >
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="android" />
                <data android:host="funtv_app" />
            </intent-filter>

            <!-- openInstall -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="mpn523" />
            </intent-filter>
        </activity>

        <service
            android:name="com.example.module_main.service.MyFirebaseMessagingService"
            android:directBootAware="true"
            android:exported="false"
            tools:targetApi="n" >
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

